 } from \"../utils/constant\";\nimport store from \"@/store\";\n\nconst whiteList = [\"/login\"];\nconst token = () => {\n  return cookie.get(COOKIE.token);\n};\n\nrouter.beforeEach((to, from, next) => {\n  if (to.path === \"/login\") {\n    store.commit(\"app/LOGIN_OFF\");\n  }\n  if (whiteList.indexOf(to.path) > -1) next();\n  else if (token()) next();\n  else next(\"/login\");\n});\n"],"sourceRoot":""}]}
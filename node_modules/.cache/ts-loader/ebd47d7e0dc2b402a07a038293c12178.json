./views/Info/Index.vue\"),\n        meta: {\n          name: \"信息列表\"\n        }\n      },\n      {\n        path: \"/info/category\",\n        name: \"category\",\n        component: () => import(\"../views/Info/Category.vue\"),\n        meta: {\n          name: \"信息分类\"\n        }\n      }\n    ]\n  },\n  {\n    path: \"/user\",\n    name: \"User\",\n    redirect: \"user-list\",\n    meta: { name: \"用户管理 \" },\n    component: Layout,\n    children: [\n      {\n        path: \"/user\",\n        name: \"user-list\",\n        component: () => import(\"../views/User/Index.vue\"),\n        meta: {\n          name: \"用户列表\"\n        }\n      }\n    ]\n  }\n];\n\nconst router = new VueRouter({\n  routes\n});\n\nexport default router;\n\nexport { routes };\n"],"sourceRoot":""}]}
A,EAAA,SAJA;AAMA,MAAA,SAAA,EAAA,SANA;AAOA,MAAA,UAAA,EAAA;AAPA,KAAA;AASA;AAlFA,CAAA","sourcesContent":["<template>\n  <div class=\"login-page\">\n    <div class=\"login-form\">\n      <ul class=\"tab\">\n        <li\n          :class=\"{ active: tab.name === activeTab }\"\n          v-for=\"tab in tabs\"\n          :key=\"tab.id\"\n          @click=\"toggleTab(tab)\"\n        >\n          {{ tab.text }}\n        </li>\n      </ul>\n      <el-form\n        :model=\"ruleForm\"\n        ref=\"ruleForm\"\n        status-icon\n        :rules=\"rules\"\n        class=\"form-body\"\n        size=\"medium\"\n      >\n        <el-form-item prop=\"email\" class=\"form-item\">\n          <label>邮箱</label>\n          <el-input\n            type=\"text\"\n            v-model=\"ruleForm.email\"\n            autocomplete=\"off\"\n          ></el-input>\n        </el-form-item>\n        <el-form-item prop=\"password\" class=\"form-item\">\n          <label>密码</label>\n          <el-input\n            type=\"password\"\n            v-model=\"ruleForm.password\"\n            autocomplete=\"off\"\n            minlength=\"6\"\n            maxlength=\"20\"\n          ></el-input>\n        </el-form-item>\n        <el-form-item\n          prop=\"rePassword\"\n          class=\"form-item\"\n          v-if=\"tabs[1].name === activeTab\"\n        >\n          <label>重复密码</label>\n          <el-input\n            type=\"password\"\n            v-model=\"ruleForm.rePassword\"\n            autocomplete=\"off\"\n            minlength=\"6\"\n            maxlength=\"20\"\n          ></el-input>\n        </el-form-item>\n        <el-form-item prop=\"authCode\" class=\"form-item\">\n          <label>验证码</label>\n          <el-row :gutter=\"10\">\n            <el-col :span=\"16\">\n              <el-input\n                v-model.number=\"ruleForm.authCode\"\n                autocomplete=\"off\"\n                maxlength=\"6\"\n                minlength=\"6\"\n              ></el-input\n            ></el-col>\n            <el-col :span=\"8\">\n              <el-button type=\"success\" class=\"btn-block\">获取验证码</el-button>\n            </el-col>\n          </el-row>\n        </el-form-item>\n        <el-form-item class=\"form-actions\">\n          <el-button type=\"danger\" @click=\"submitForm('ruleForm')\"\n            >提交</el-button\n          >\n        </el-form-item>\n      </el-form>\n    </div>\n  </div>\n</template>\n<script>\nimport validation from \"../../utils/validation.rule\";\nimport { reactive, ref } from \"@vue/composition-api\";\nexport default {\n  name: \"login\",\n  setup(props, context) {\n    console.log(props, context);\n    const rulePwd = (rule, value, callback) => {\n      const reg = /^(?!\\D+$)(?![^a-zA-Z]+$)\\S{6,20}$/g;\n      if (!value) callback();\n      else if (!reg.test(value)) {\n        callback(new Error(\"密码为6-20位的数字和字母\"));\n      } else callback();\n    };\n    const ruleCode = (rule, value, callback) => {\n      const reg = /^[0-9a-zA-Z]{6}$/g;\n      if (!value || !reg.test(value)) callback(new Error(\"请输入验证码\"));\n      else callback();\n    };\n    const _form = {\n      email: \"\",\n      password: \"\",\n      rePassword: \"\",\n      authCode: \"\"\n    };\n    const _tabs = [\n      { name: \"login\", text: \"登陆\" },\n      { name: \"signup\", text: \"注册\" }\n    ];\n    const tabs = reactive(_tabs),\n      activeTab = ref(\"login\"),\n      ruleForm = reactive(_form),\n      rules = reactive({\n        email: [\n          { validator: validation.email, trigger: \"blur\" },\n          { required: true, message: \"请输入邮箱\", trigger: \"blur\" }\n        ],\n        password: [\n          { validator: rulePwd, trigger: \"blur\" },\n          { required: true, message: \"请输入密码\", trigger: \"blur\" }\n        ],\n        rePassword: [\n          { required: true, message: \"请再次输入密码\", trigger: \"blur\" },\n          {\n            validator: (rule, value, callback) => {\n              if (value.isNullorEmpty()) callback();\n              else if (!tabs[1].name === this.activeTab.value) callback();\n              else if (ruleForm.password == value) callback();\n              else callback(new Error(\"两次输入密码不一致\"));\n            },\n            message: \"两次输入密码不一致\",\n            trigger: \"blur\"\n          }\n        ],\n        authCode: [\n          { validator: ruleCode, trigger: \"blur\" },\n          { required: true, message: \"请输入验证码\", trigger: \"blur\" }\n        ]\n      });\n    const toggleTab = tab => {\n      tabs.forEach(item => (item.selected = false));\n      activeTab.value = tab.name;\n    };\n    // eslint-disable-next-line prettier/prettier\n    const submitForm = function (formName) {\n      console.log(props, context);\n      console.log(formName);\n\n      ruleForm.validate(valid => {\n        if (valid) alert(\"submit\");\n        else {\n          console.log(\"error submit\");\n          return false;\n        }\n      });\n    };\n    return {\n      ruleForm,\n      rules,\n      tabs,\n      activeTab,\n\n      toggleTab,\n      submitForm\n    };\n  }\n};\n</script>\n<style lang=\"scss\" scoped>\n$backgroud: #344a5f;\n.login-page {\n  background-color: $backgroud;\n  height: 100vh;\n  .login-form {\n    width: 330px;\n    margin: auto;\n    .tab {\n      text-align: center;\n      li {\n        display: inline-block;\n        width: 88px;\n        height: 36px;\n        line-height: 36px;\n        font-size: 14px;\n        border-radius: 2px;\n        color: #fff;\n        cursor: pointer;\n        &.active {\n          background-color: darken($color: $backgroud, $amount: 10%);\n        }\n        &:hover:not(.active) {\n          background-color: darken($color: $backgroud, $amount: 8%);\n        }\n      }\n    }\n    .form-body {\n      margin-top: 29px;\n      .btn-block {\n        display: block;\n        width: 100%;\n      }\n      .form-item {\n        margin-bottom: 13px;\n        label {\n          display: block;\n          font-size: 14px;\n          color: #fff;\n          margin-bottom: 3px;\n        }\n      }\n      .form-actions {\n        margin-top: 19px;\n        button {\n          @extend .btn-block;\n        }\n      }\n    }\n  }\n}\n</style>\n"],"sourceRoot":"src/views/Login"}]}
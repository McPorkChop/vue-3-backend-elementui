AFA,EAEA,OAFA,EAEA;AACA,QAAA,OAAA,GAAA,EAAA,CADA,CAEA;;AACA,IAAA,OAAA,GAAA,UAAA,CAAA,IAAA,CAAA,OAAA,CAAA,CAHA,CAIA;;AACA,IAAA,OAAA,GAAA,UAAA,CAAA,IAAA,CAAA,OAAA,EAAA,OAAA,CAAA,CALA,CAOA;;AACA,WAAA,OAAA;AACA;AAXA,CAAA","sourcesContent":["<template>\n  <div class=\"login-page\">\n    <div class=\"login-form\">\n      <ul class=\"tab\">\n        <li\n          :class=\"{ active: tab.name === activeTab }\"\n          v-for=\"tab in tabs\"\n          :key=\"tab.id\"\n          @click=\"toggleTab(tab)\"\n        >\n          {{ tab.text }}\n        </li>\n      </ul>\n      <el-form\n        :model=\"form\"\n        ref=\"form\"\n        status-icon\n        :rules=\"rules\"\n        class=\"form-body\"\n        size=\"medium\"\n      >\n        <el-form-item prop=\"email\" class=\"form-item\">\n          <label>邮箱</label>\n          <el-input\n            type=\"text\"\n            v-model=\"form.email\"\n            auto-complete=\"off\"\n          ></el-input>\n        </el-form-item>\n        <el-form-item prop=\"password\" class=\"form-item\">\n          <label>密码</label>\n          <el-input\n            type=\"password\"\n            v-model=\"form.password\"\n            auto-complete=\"off\"\n            minlength=\"6\"\n            maxlength=\"20\"\n          ></el-input>\n        </el-form-item>\n        <el-form-item\n          prop=\"rePassword\"\n          class=\"form-item\"\n          v-if=\"tabs[1].name === activeTab\"\n        >\n          <label>重复密码</label>\n          <el-input\n            type=\"password\"\n            v-model=\"form.rePassword\"\n            auto-complete=\"off\"\n            minlength=\"6\"\n            maxlength=\"20\"\n          ></el-input>\n        </el-form-item>\n        <el-form-item prop=\"authCode\" class=\"form-item\">\n          <label>验证码</label>\n          <el-row :gutter=\"10\">\n            <el-col :span=\"16\">\n              <el-input\n                v-model=\"form.authCode\"\n                auto-complete=\"off\"\n                maxlength=\"6\"\n                minlength=\"6\"\n              ></el-input>\n            </el-col>\n            <el-col :span=\"8\">\n              <el-button\n                type=\"success\"\n                class=\"btn-block\"\n                @click=\"getCode\"\n                :disabled=\"sendCode > 0\"\n                >{{ sendCode ? `等待${sendCode}秒` : \"发送验证码\" }}</el-button\n              >\n            </el-col>\n          </el-row>\n        </el-form-item>\n        <el-form-item class=\"form-actions\">\n          <el-button type=\"danger\" @click=\"submit\" :disabled=\"!submitable\">\n            {{ activeTab === \"login\" ? \"登陆\" : \"注册\" }}\n          </el-button>\n        </el-form-item>\n      </el-form>\n    </div>\n  </div>\n</template>\n<script>\nimport validation from \"util/validation.rule\";\nimport { reactive, ref } from \"@vue/composition-api\";\nimport { authCode, register, login } from \"../../api/login\";\n\nfunction defineProp() {\n  (this.activeTab = ref(\"login\")),\n    (this.submitable = ref(false)),\n    (this.requiring = ref(false)),\n    (this.sendCode = ref(0)),\n    (this.tabs = reactive([\n      { name: \"login\", text: \"登陆\" },\n      { name: \"signup\", text: \"注册\" }\n    ]));\n  this.form = reactive({\n    email: \"\",\n    password: \"\",\n    rePassword: \"\",\n    authCode: \"\"\n  });\n  this.rules = reactive({\n    email: [\n      { validator: validation.email, trigger: \"blur\" },\n      { required: true, message: \"请输入邮箱\", trigger: \"blur\" }\n    ],\n    password: [\n      {\n        validator: (rule, value, callback) => {\n          const reg = /^(?!\\D+$)(?![^a-zA-Z]+$)\\S{6,20}$/g;\n          if (!value) callback();\n          else if (!reg.test(value)) {\n            callback(new Error(\"密码为6-20位的数字和字母\"));\n          } else callback();\n        },\n        trigger: \"blur\"\n      },\n      { required: true, message: \"请输入密码\", trigger: \"blur\" }\n    ],\n    rePassword: [\n      { required: true, message: \"请再次输入密码\", trigger: \"blur\" },\n      {\n        validator: (rule, value, callback) => {\n          if (!value) callback();\n          else if (!this.tabs[1].name === this.activeTab.value) callback();\n          else if (this.form.password == value) callback();\n          else callback(new Error(\"两次输入密码不一致\"));\n        },\n        message: \"两次输入密码不一致\",\n        trigger: \"blur\"\n      }\n    ],\n    authCode: [\n      {\n        validator: (rule, value, callback) => {\n          const reg = /^[0-9a-zA-Z]{6}$/g;\n          if (!value || !reg.test(value)) callback(new Error(\"请输入验证码\"));\n          else callback();\n        },\n        trigger: \"blur\"\n      },\n      { required: true, message: \"请输入验证码\", trigger: \"blur\" }\n    ]\n  });\n  return this;\n}\n\nfunction defineFunc(context) {\n  const { root, refs } = context;\n  this.toggleTab = tab => {\n    this.activeTab.value = tab.name;\n    refs.form.resetFields();\n    this.submitable.value = false;\n  };\n  this.countDownSend = () => {\n    this.sendCode.value--;\n    if (this.sendCode.value > 0) setTimeout(this.countDownSend, 1000);\n  };\n  this.getCode = () => {\n    const fields = [\"email\", \"password\"];\n    if (this.activeTab.value === \"signup\") fields.push(\"rePassword\");\n    const _method = this.activeTab.value === \"login\" ? \"login\" : \"register\";\n\n    let _promise = Promise.resolve(),\n      _success = true;\n    refs.form.validateField(fields, error => {\n      if (error) {\n        _promise = _promise\n          .then(root.$nextTick)\n          .then(() => root.$message.error(error));\n        _success = false;\n        return;\n      }\n    });\n    if (!_success) return;\n    this.sendCode.value = 120;\n\n    authCode(this.form.email, _method)\n      .then(response => {\n        console.log(response);\n        this.submitable.value = true;\n        root.$message.success(response.data.message);\n        this.countDownSend();\n      })\n      .catch(error => {\n        console.log(error);\n        this.submitable.value = false;\n        root.$message.success(error.message);\n      });\n  };\n  this.submit = () => {\n    refs.form.validate(valid => {\n      if (valid) {\n        if (this.activeTab === \"signup\")\n          register(this.form.email, this.form.password, this.form.authCode)\n            .then(response => {\n              this.submitable.value = true;\n              root.$message.success(response.data.message);\n            })\n            .catch(error => {\n              this.submitable.value = true;\n              root.$message.error(error.message);\n            });\n        else\n          login(this.form.email, this.form.password, this.form.authCode)\n            .then(response => {\n              root.$message.success(response.data.message);\n              root.$router.push({\n                name: \"dashboard\"\n              });\n            })\n            .catch(error => {\n              root.$message.error(error.message);\n            });\n      } else {\n        console.log(\"error submit\");\n        return false;\n      }\n    });\n  };\n  return this;\n}\n\nexport default {\n  name: \"login\",\n  setup(props, context) {\n    let _define = {};\n    //属性定义\n    _define = defineProp.call(_define);\n    //方法定义\n    _define = defineFunc.call(_define, context);\n\n    //返回定义\n    return _define;\n  }\n};\n</script>\n<style lang=\"scss\" scoped>\n$backgroud: #344a5f;\n.login-page {\n  background-color: $backgroud;\n  height: 100vh;\n  .login-form {\n    width: 330px;\n    margin: auto;\n    .tab {\n      text-align: center;\n      li {\n        display: inline-block;\n        width: 88px;\n        height: 36px;\n        line-height: 36px;\n        font-size: 14px;\n        border-radius: 2px;\n        color: #fff;\n        cursor: pointer;\n        &.active {\n          background-color: darken($color: $backgroud, $amount: 10%);\n        }\n        &:hover:not(.active) {\n          background-color: darken($color: $backgroud, $amount: 8%);\n        }\n      }\n    }\n    .form-body {\n      margin-top: 29px;\n      .btn-block {\n        display: block;\n        width: 100%;\n      }\n      .form-item {\n        margin-bottom: 13px;\n        label {\n          display: block;\n          font-size: 14px;\n          color: #fff;\n          margin-bottom: 3px;\n        }\n      }\n      .form-actions {\n        margin-top: 19px;\n        button {\n          @extend .btn-block;\n        }\n      }\n    }\n  }\n}\n</style>\n"],"sourceRoot":"src/views/Login"}]}